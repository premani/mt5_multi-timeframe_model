# MT5 LSTM Model - 環境変数設定テンプレート
# このファイルを .env にコピーして使用してください

# MT5 API Server設定
MT5_API_BASE_URL=http://localhost:5000

# MT5接続情報（API Server側で使用）
# MT5_LOGIN=your_login_id
# MT5_PASSWORD=your_password
# MT5_SERVER=your_broker_server

# タイムゾーン設定（デフォルト: Asia/Tokyo）
TZ=Asia/Tokyo

# デバッグモード（開発時のみ true に設定）
DEBUG=false

# ログレベル（DEBUG, INFO, WARNING, ERROR）
LOG_LEVEL=INFO
